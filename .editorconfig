;,.editorconfig
root = true
;edit
[**.js]
path=~/.cache/vimfiles/repos/github.com/maksimr/vim-jsbeautify/plugin/lib/js/lib/beautify.js
;vim:bin=node
brace_style = collapse,preserve-inline
e4x = true
indent_style = space
indent_size = 2

[**.json]
indent_style = space
indent_size = 2

[**.jsx]
path=~/.cache/vimfiles/repos/github.com/maksimr/vim-jsbeautify/plugin/lib/js/lib/beautify.js
;vim:bin=node
e4x=true
indent_style = space
indent_size = 2

[**.css]
indent_style = space
indent_size = 2

[**.html]
indent_style = space
indent_size = 2
max_char = 78
brace_style = expand
